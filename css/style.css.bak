@charset "utf-8";
@import url('https://fonts.googleapis.com/css?family=Indie+Flower');
@import url('https://fonts.googleapis.com/css?family=Allura');
*{margin:0; padding:0;}
ul,ol {list-style:none;}
a{text-decoration:none; color:#222;}
img{border:0;vertical-align:top;}
body{font-size:14px; font-family: "Crimson Text","맑은 고딕";color:#fff; min-width:1500px;}
header,footer,section{display:block;}


.main_logo{position:absolute; top:38%; left:51.5%; margin-left:-40px; z-index:999999; width:80px; height:80px;}
.main_logo  img{width:100%; height:auto;}


#nav{position:fixed;  top:0; left:0; width:15%; height:100%; z-index:99999;}
#sns{position:fixed; top:30px; right:30px; z-index:9999;}
#sns ul li{float:left; width:50px;}
#sns ul li i{color:#fff;font-size:25px; transition:all 0.5s;}
#sns ul li i:hover{transform:scale(1.4);}
/* section01 첫번째 이미지 */
#section01{position:relative; width:100%; background:url(../images/home_bg_far.jpg)no-repeat; overflow:hidden;}
#section01 .move_image {position:absolute;top:-50px;left:20px; z-index:9999;}
#section01 div img{width:100%;}
#section01 h2, #section01 p{position:absolute; z-index:999;}
#section01 h2{left:50%;margin-left:-200px; top:47%;font-size:35px; font-family: 'Indie Flower', cursive;}
#section01 p {left:30%; top:52%;font-size:26px; font-family: 'Indie Flower', cursive;}

/* section02 두번째이미지 */
#section02{position:relative;background:rgba(0,0,0,0.5); z-index:999;}
#section02 .txt{position:absolute; top:265px; left:50%; margin-left:-300px;width:700px;}
#section02 .txt p{font-size:26px; font-family: 'Indie Flower', cursive; letter-spacing:1px;}

/* section03 세번째이미지 */
#section03{position:relative;width:100%;background:#cde4b0; z-index:999;}
