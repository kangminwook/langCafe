#section03 .txt h2{font-size:40px;}
#section03 .txt p{font-size:22px;}

#section03 div.menu{position:absolute; left:-30%; top:25%; margin-left:-600px;width:1200px; height:300px; }
#section03 div ul li{display:inline-block; width:24%; text-align:center; vertical-align:middle;}
#section03 div ul li a img{transition:all 0.8s;}
#section03 div ul li:hover img{transform:rotateY(180deg);}
#section03 div.menu_item{display:none; position:fixed; width:100%;  height:100%; top:0;left:0;}
#section03 div.menu_item .close{position:absolute;top:20px; right:20px; cursor:pointer;}
#section03 div.menu_item .close img{width:100px; height:100px;}
#section03 div.menu_item .menu_info{width:1000px; height:500px; position:absolute; top:20%; left:50%; margin-left:-500px;}
#section03 div.menu_item .menu_info img{float:left;}
#section03 div.menu_item .menu_info p{float:left;width:450px; font-size:20px; padding-top:150px; padding-left:100px;}


